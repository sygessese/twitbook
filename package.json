{
  "name": "twitook",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run prod",
    "build": "npm run clean && npm run transpile",
    "dev-server": "webpack -d & node ./dist/index.js",
    "prod-server": "webpack --mode production & node ./dist/index.js",
    "dev": "NODE_ENV=development npm run build && npm run dev-server",
    "prod": "NODE_ENV=production npm run build  && npm run prod-server",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch:dev": "nodemon"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "flux": "^3.1.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.4",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-mixin": "^5.0.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "reqwest": "^2.0.5",
    "rimraf": "^3.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "serve-favicon": "^2.5.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "when": "^3.7.8",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "styled-components": "^5.0.1"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "client/src/"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  }
}
